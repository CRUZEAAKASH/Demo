<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
    <parameter name="browser" value="chrome"></parameter>
    <test thread-count="5" name="Amazon TestNG xml">
        <classes>
            <class name="AmazonTest"/>
        </classes>
    </test> <!-- Test -->
    <test name="Green Kart TestNG xml">
        <classes>
            <class name="GreenKartTest">
                <methods>
                    <exclude name=".*Excluded"></exclude>
                </methods>
            </class>
        </classes>
    </test>
</suite> <!-- Suite -->
